<% if !current_user %>
    <div class = "outer_container">
        <div class = "inner_container">
            <%= link_to "Sign in with Google", "/auth/google_oauth2", id: "sign_in", class: "btn btn-primary" %>
        </div>
    </div>
<% else %>
    <div class = "jumbotron">
        <% if current_user %>
            <div class = "col-md-8" id = "calendar">cal</div>
            <div class = "col-md-4" id = "todolist">todolist</div>
        <% end %>
    </div>
<% end %>